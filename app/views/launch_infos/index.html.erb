<h1 class="text-center">All launches</h1>
<div class="container">
  <div class="row justify-content-md-center">
    <div class="col-md-4">
      <% @launches.each do |launch| %>
      <div class="card" >
        <div class="card-body">
          <h5 class="card-title"><%= launch %></h5>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>
